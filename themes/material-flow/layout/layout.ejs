<%- partial('_partial/head.ejs') %>
<body>
  <%- partial('_partial/loading.ejs') %>
  <script>setLoadingBarProgress(20)</script> 
  <%- partial('_partial/header.ejs', null, {cache: !config.relative_link}) %>
    <script>setLoadingBarProgress(40);</script>
  <div class="l_body">
    <div class='container clearfix'>
      <div class='l_main'>
        <%- body %>
      </div>
      <aside class='l_side'>
        <%- partial('_partial/side.ejs') %>
      </aside>
      <script>setLoadingBarProgress(60);</script>
    </div>
  </div>
  <%- partial('_partial/footer.ejs', null, {cache: !config.relative_link}) %>
  <script>setLoadingBarProgress(80);</script>
  <%- partial('_partial/scripts.ejs') %>
  <script>setLoadingBarProgress(100);</script>
</body>
</html>
